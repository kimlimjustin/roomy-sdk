{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowImportingTsExtensions": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "exactOptionalPropertyTypes": true,
    "strictNullChecks": true,
    "lib": [
      "ESNext",
      "DOM"
    ],
    "paths": {
      "@muni-town/*": [
        "./node_modules/@muni-town/*"
      ],
      "npm:loro-crdt@^1.5.1": [
        "./node_modules/.deno/loro-crdt@1.5.3/node_modules/loro-crdt/bundler"
      ]
    },
    "types": [
      "./types/extra.d.ts",
      "./types/deno/lib.esnext",
      "./types/deno/lib",
      "./types/deno/lib.deno.ns",
      "./types/deno/lib.deno.unstable",
    ]
  },
  "include": [
    "../src"
  ],
}